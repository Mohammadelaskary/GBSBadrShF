<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".warhouse.WarehousesMain">
    <com.google.android.material.button.MaterialButton
        android:id="@+id/counting_btn"
        style="@style/MenuButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="70dp"
        android:backgroundTint="@color/appbarcolor"
        app:cornerRadius="25dp"
        android:text="@string/counting"

        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/warehousereciving_btn"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/counting_btn"

        android:layout_marginTop="15dp"
        android:backgroundTint="@color/appbarcolor"
        app:cornerRadius="25dp"
        android:text="@string/warehouse_receiving"
        style="@style/MenuButton"

        />





</androidx.constraintlayout.widget.ConstraintLayout>