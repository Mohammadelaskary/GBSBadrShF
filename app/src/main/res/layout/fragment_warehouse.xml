<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".warhouse.warehouse.WarehouseFragment">
    <TextView
        android:id="@+id/productionsignoff_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        style="@style/styleoftextinappbar"
        android:text="Warehouse reciving"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"
        />
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/productionsignoff_txt"
        app:layout_constraintBottom_toBottomOf="parent"
        android:fillViewport="true"

        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="15dp"

            >
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/barcodecode_edt"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                app:startIconDrawable="@drawable/ic_barcode_icon"
                app:errorEnabled="true"
                app:errorTextColor="@android:color/holo_red_dark"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/barcodenew_edt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Barcode"
                    android:imeOptions="actionGo"
                    android:maxLines="1"
                    android:lines="1"
                    android:singleLine="true"
                    android:textColor="@color/colorPrimary" />
            </com.google.android.material.textfield.TextInputLayout>

<!--            <TextView-->
<!--                android:id="@+id/barcodecode_txt"-->
<!--                android:layout_width="wrap_content"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:text="@string/barcode"-->
<!--                style="@style/styleoftextbold"-->
<!--                app:layout_constraintStart_toStartOf="parent"-->
<!--                android:layout_marginStart="15dp"-->
<!--                app:layout_constraintTop_toTopOf="parent"-->
<!--                />-->
<!--            <EditText-->
<!--                android:id="@+id/barcode_edt"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="50dp"-->
<!--                android:layout_marginTop="15dp"-->
<!--                android:layout_marginStart="15dp"-->
<!--                android:layout_marginEnd="55dp"-->
<!--                app:layout_constraintStart_toStartOf="@id/barcodecode_txt"-->
<!--                app:layout_constraintTop_toBottomOf="@id/barcodecode_txt"-->
<!--                android:background="@drawable/edt_bg"-->
<!--                android:textColor="@color/black"-->

<!--                />-->

            <TextView
                android:id="@+id/parentcode_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Locator : "
                style="@style/styleoftextbold"
                app:layout_constraintStart_toStartOf="@id/barcodecode_edt"
                app:layout_constraintTop_toBottomOf="@id/barcodecode_edt"
                />
            <TextView
                android:id="@+id/parentcode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginStart="2dp"
                style="@style/styleoftext"
                app:layout_constraintStart_toEndOf="@id/parentcode_txt"
                app:layout_constraintTop_toTopOf="@id/parentcode_txt"
                android:layout_marginTop="2dp"
                />
            <TextView
                android:id="@+id/parentdesc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginTop="10dp"
                style="@style/styleoftext"
                app:layout_constraintStart_toStartOf="@id/parentcode"
                app:layout_constraintTop_toBottomOf="@+id/parentcode"
                />
            <TextView
                android:id="@+id/jobordernum_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/parentdesc"
                app:layout_constraintStart_toStartOf="@id/parentcode_txt"
                android:layout_marginTop="15dp"
                android:text="@string/job_order_number_txt"
                style="@style/styleoftextbold"
                />
            <TextView
                android:id="@+id/jobordernum"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text=""
                style="@style/styleoftext"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                app:layout_constraintTop_toTopOf="@id/jobordernum_txt"
                app:layout_constraintStart_toEndOf="@id/jobordernum_txt"
                />
            <TextView
                android:id="@+id/paintquantity_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginStart="15dp"
                style="@style/styleoftextbold"
                android:text="Count Qty : "
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/jobordernum"
                />
            <TextView
                android:id="@+id/paintqtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                style="@style/styleoftext"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                app:layout_constraintTop_toTopOf="@id/paintquantity_txt"
                app:layout_constraintStart_toEndOf="@id/paintquantity_txt"
                />
            <TextView
                android:id="@+id/subinventory_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sub Inventory : "
                style="@style/styleoftextbold"
                app:layout_constraintStart_toStartOf="@id/paintquantity_txt"
                app:layout_constraintTop_toBottomOf="@id/paintquantity_txt"
                android:layout_marginTop="20dp"
                />
            <TextView
                android:id="@+id/subinventorycode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginStart="2dp"
                style="@style/styleoftext"
                app:layout_constraintStart_toEndOf="@id/subinventory_txt"
                app:layout_constraintTop_toTopOf="@id/subinventory_txt"
                android:layout_marginTop="2dp"
                />
            <TextView
                android:id="@+id/subinventorydesc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginTop="10dp"
                style="@style/styleoftext"
                app:layout_constraintStart_toStartOf="@id/subinventorycode"
                app:layout_constraintTop_toBottomOf="@+id/subinventorycode"
                />

            <TextView
                android:id="@+id/qty_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Qty"
                style="@style/styleoftextbold"
                app:layout_constraintStart_toStartOf="@id/paintquantity_txt"
                android:layout_marginTop="15dp"
                app:layout_constraintTop_toBottomOf="@id/subinventorydesc"
                />

            <EditText
                android:id="@+id/qty_edt"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/edt_bg"
                android:textColor="@color/black"
                android:imeOptions="actionDone"
                android:maxLines="1"
                android:lines="1"
                android:singleLine="true"
                app:layout_constraintEnd_toEndOf="@+id/barcodecode_edt"
                app:layout_constraintStart_toStartOf="@id/qty_txt"
                app:layout_constraintTop_toBottomOf="@id/qty_txt" />




            <com.google.android.material.button.MaterialButton
                android:id="@+id/save_btn"
                android:layout_width="200dp"
                android:layout_height="55dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/qty_edt"
                app:layout_constraintBottom_toBottomOf="parent"
                android:backgroundTint="@color/appbarcolor"
                android:layout_marginTop="15dp"
                app:cornerRadius="25dp"
                android:text="@string/save"
                style="@style/styleoftextinbtn"

                />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>




</androidx.constraintlayout.widget.ConstraintLayout>