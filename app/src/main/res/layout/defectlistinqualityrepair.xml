<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <CheckBox
        android:id="@+id/defectname_checkBox"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="shaseh"
        android:padding="8dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/child"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/defectname_checkBox"
        app:layout_constraintTop_toTopOf="@id/defectname_checkBox"
        app:layout_constraintEnd_toStartOf="@+id/defectqty"
        android:layout_marginStart="80dp"
        android:layout_marginTop="2dp"
        android:text="@string/parentdes_txt"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/defectqty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/child"
        app:layout_constraintEnd_toStartOf="@id/repairedqty"
        app:layout_constraintTop_toTopOf="@id/child"
        android:text="155"
        android:layout_marginStart="50dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/repairedqty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/child"
        app:layout_constraintStart_toEndOf="@+id/defectqty"
        app:layout_constraintEnd_toStartOf="@id/approvedqty"
        android:text="120"
        android:layout_marginStart="120dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />

    <TextView
        android:id="@+id/approvedqty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/child"
        app:layout_constraintStart_toEndOf="@+id/repairedqty"
        app:layout_constraintEnd_toStartOf="@id/notes"
        android:text="110"
        android:layout_marginStart="120dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/notes"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/child"
        app:layout_constraintStart_toEndOf="@+id/approvedqty"
        android:layout_marginStart="90dp"
        android:text="No problem Found"
        android:textSize="16sp"
        style="@style/styleoftext"
        />








</androidx.constraintlayout.widget.ConstraintLayout>